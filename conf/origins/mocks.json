{
    "id": "mocks",
    "prefix": "/mocks",
    "logLevel": "warn",
    "exposeErrors": true,
    "redis": {
        "socket": {
            "host": "redis",
            "port": 6379
        }
    },
    "origin": {
        "http2": false,
        "requestCoalescing": true,
        "circuitBreaker": true,
        "fetchTimeout": 2000,
        "ignoredQueryParams": ["cc", "delay"],
        "sortQueryParams": true,
        "httpxOptions": {
            "protocol": "http:",
            "host": "mocks",
            "family": 4,
            "port": 3030,
            "method": "GET",
            "headers": {},
            "timeout": 2000
        },
        "agentOptions": {
            "keepAlive": true
        },
        "circuitBreakerOptions": {
            "errorThresholdPercentage": 10,
            "resetTimeout": 15000
        },
        "transformations": []
    }
}