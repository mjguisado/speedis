{
    "id": "mocks",
    "prefix": "/mocks",
    "logLevel": "warn",
    "exposeErrors": false,
    "redis": {
        "url": "redis://127.0.0.1:6379"
    },
    "origin": {
        "httpxOptions": {
            "protocol": "http:",
            "host": "mocks",
            "family": 4,
            "port": 3030,
            "method": "GET",
            "headers": {},
            "timeout": 2000
        },
        "agentOptions": {
            "keepAlive": true
        },
        "fetchTimeout": 2000,
        "ignoredQueryParams": ["cc", "delay"],
        "sortQueryParams": true,
        "requestCoalescing": true,
        "lock": true,
        "lockOptions": {
            "lockTTL": 2500,
            "retryCount": 3,
            "retryDelay": 500,
            "retryJitter": 100
        },
        "circuitBreaker": true,
        "circuitBreakerOptions": {
            "errorThresholdPercentage": 10,
            "resetTimeout": 15000
        },
        "transformations": []
    }
}