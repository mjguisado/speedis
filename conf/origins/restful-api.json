{
    "id": "restful-api",
    "prefix": "/restful-api",
    "logLevel": "info",
    "exposeErrors": true,
    "origin": {
        "http2": false,
        "httpxoptions": {
            "protocol": "https:",
            "hostname": "api.restful-api.dev",
            "port": 443,
            "method": "GET",
            "headers": {
                "Content-Type": "application/json"
            },
            "timeout": 2000
        }
    },
    "agentOpts": {
        "keepAlive": true
    },
    "redisOpts": {
        "socket": {
            "host": "127.0.0.1",
            "port": 6379
        }
    },
    "mutations": [
        {
            "urlPattern": ".*",
            "actions": [
                {
                    "phase": "OriginResponse",
                    "func": "setHeaders",
                    "params": {
                        "cache-control": "public, s-maxage=60, max-age=15"
                    }
                }
            ]
        }
    ]
}